<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v2.dtd">
<config>

	<module name="global">
		<param name="randomSeed" value="4711" />
		<param name="coordinateSystem" value="epsg:5513" />
		<param name="numberOfThreads" value="2" /> <!-- parallel event handling -->
	</module>

	<module name="network">
		<param name="inputNetworkFile" value="./input/pt-network-prague.xml.gz" />
	</module>

	<module name="plans">
		<param name="inputPlansFile" value="./input/test-population-2.xml" />
	</module>

	<module name="controler">
		<param name="outputDirectory" value="./output/pop2-eh2-qsim2" />
		<param name="firstIteration" value="0" />
		<param name="lastIteration" value="10" />
		<param name="compressionType" value="gzip" />
		<param name="dumpDataAtEnd" value="true" />
		<param name="overwriteFiles" value="deleteDirectoryIfExists" />

	</module>

	<module name="qsim">
		<!-- "start/endTime" of MobSim (00:00:00 == take earliest activity time/ run as long as active vehicles exist) -->
		<param name="startTime" value="00:00:00" />
		<param name="endTime" value="30:00:00" />
		<param name="flowCapacityFactor" value="0.1" /> <!-- simulating 10% population sample -->
		<param name="storageCapacityFactor" value="0.3" />
		<param name="snapshotperiod"  value = "5:00:00"/> <!-- 00:00:00 means NO snapshot writing -->
		<param name="mainMode" value="car,bus,tram,subway,rail" />
		<param name="numberOfThreads" value="2" /> <!-- Parallel QSim -->
	</module>

	<module name="transit" >
		<param name="useTransit" value="true" />
		<param name="transitScheduleFile" value="./input/transit-schedule.xml.gz" />
		<param name="vehiclesFile" value="./input/01-transit-vehicles.xml.gz" />
		<param name="transitModes" value="pt" />
	</module>

	<module name="planCalcScore">
		<param name="learningRate" value="1.0" />
		<param name="BrainExpBeta" value="2.0" /> <!-- The default value is 2 for historical reasons, but it should be set to 1 if the parameters of the scoring function are estimated rather than hand-calibrated. -->
		<param name="lateArrival" value="-18" />
		<param name="earlyDeparture" value="-0" />
		<param name="performing" value="+6" /> <!-- normally a positive value (since performing an activity for more time normally increases utility) -->
		<param name="waiting" value="-0" />
		<param name="marginalUtilityOfMoney" value="1.0" /> <!-- normally a positive value (since having more money normally increases utility) -->


		<parameterset type="modeParams" >
			<param name="mode" value="car" />
			<param name="constant" value="-4.0" />
			<param name="dailyMonetaryConstant" value="-5.3" />
			<param name="dailyUtilityConstant" value="0.0" />
			<param name="marginalUtilityOfDistance_util_m" value="0.0" />
			<param name="marginalUtilityOfTraveling_util_hr" value="-6.0" />
			<param name="monetaryDistanceRate" value="-0.0002" /> <!-- has to be negative -->
		</parameterset>
		<parameterset type="modeParams" >
			<param name="mode" value="pt" />
			<param name="constant" value="-0.5" />
			<param name="marginalUtilityOfDistance_util_m" value="0.0" />
			<param name="marginalUtilityOfTraveling_util_hr" value="-0.5" />
			<param name="monetaryDistanceRate" value="0.0" />
		</parameterset>
		<parameterset type="modeParams" >
			<param name="mode" value="walk" />
			<param name="constant" value="0.0" />
			<param name="marginalUtilityOfDistance_util_m" value="0.0" />
			<param name="marginalUtilityOfTraveling_util_hr" value="-6.0" />
			<param name="monetaryDistanceRate" value="0.0" />
		</parameterset>
		<parameterset type="activityParams" >
			<param name="activityType"            value="home" /> <!-- home -->
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="12:00:00" />
		</parameterset>
		<parameterset type="activityParams" >
			<param name="activityType"            value="work" /> <!-- work -->
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="08:00:00" />
			<param name="openingTime"     value="07:00:00" />
			<param name="latestStartTime" value="13:00:00" />
			<param name="earliestEndTime" value="6:00:00" />
			<param name="closingTime"     value="22:00:00" />
		</parameterset>
		<param name="writeExperiencedPlans" value="true" />
	</module>
	
	<module name="strategy">
		<param name="maxAgentPlanMemorySize" value="5" /> <!-- 0 means unlimited -->

		<param name="ModuleProbability_1" value="0.9" />
		<param name="Module_1" value="BestScore" />

		<param name="ModuleProbability_2" value="0.1" />
		<param name="Module_2" value="ReRoute" />

		<param name="ModuleProbability_3" value="0.1" />
		<param name="Module_3" value="ChangeTripMode" />
	</module>

	<module name="changeMode">
		<param name="modes" value="car,pt" />
	</module>

</config>
